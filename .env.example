# N8N Configuration
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=seu_usuario
N8N_BASIC_AUTH_PASSWORD=sua_senha_forte

# Database (Railway irá fornecer)
DATABASE_URL=postgresql://user:password@host:port/database
# DATABASE_URL=${{DATABASE_URL}}

# Webhooks (Railway irá fornecer o domínio)
WEBHOOK_URL=https://seu-app.railway.app/
# WEBHOOK_URL=https://${{RAILWAY_STATIC_URL}}/
N8N_HOST=seu-app.railway.app
# N8N_HOST=${{RAILWAY_STATIC_URL}}
N8N_PORT=5678
N8N_PROTOCOL=https

# Timezone
GENERIC_TIMEZONE=America/Sao_Paulo
TZ=America/Sao_Paulo

# Security
N8N_SECURE_COOKIE=true
N8N_FORCE_SSL=true
